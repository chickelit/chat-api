    "eslint-plugin-prettier": "^4.0.0",
    "execa": "^5.1.1",
    "get-port": "^5.1.1",
    "japa": "^3.1.1",
    "pino-pretty": "^7.0.1",
    "prettier": "^2.4.1",
    "supertest": "^6.1.6",
    "typescript": "~4.2",
